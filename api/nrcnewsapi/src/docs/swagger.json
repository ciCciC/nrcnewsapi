    "basePath": "/api/v1",
    "paths": {
        "/category/technology": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "Retrieves all Technology articles",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/model.ArticleItem"
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "model.ArticleItem": {
            "type": "object",
            "properties": {
                "imageLink": {
                    "type": "string",
                    "example": "page link"
                },
                "pageLink": {
                    "type": "string",
                    "example": "page link"
                },
                "teaser": {
                    "type": "string",
                    "example": "page link"
                },
                "title": {
                    "type": "string",
                    "example": "page link"
                },
                "topic": {
                    "type": "string",
                    "example": "page link"
                }
            }
        }
    }
}